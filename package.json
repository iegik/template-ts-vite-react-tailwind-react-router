{
  "name": "template-ts-vite-react-tailwind-react-router",
  "version": "0.0.0",
  "author": {
    "name": "Fabien MARIE-LOUISE",
    "url": "https://github.com/fabien-ml"
  },
  "type": "module",
  "scripts": {
    "build": "npm run typecheck && vite build",
    "commit": "git-cz",
    "dev": "vite",
    "format": "prettier . --write --ignore-path .gitignore && git update-index --again",
    "preinstall": "npx solidarity",
    "lint": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "prepare": "husky",
    "serve": "npm run build && vite preview --port 3000",
    "test:e2e": "start-server-and-test serve http://127.0.0.1:3000/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test serve http://127.0.0.1:3000/ 'cypress run'",
    "test:unit": "jest --watch",
    "test:unit:ci": "jest",
    "test:unit:coverage": "jest --silent --watchAll=false --coverage",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{css,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --write"
    ],
    "package.json": "npx sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@cypress/vite-dev-server": "^5.1.1",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/testing-library__user-event": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cypress": "^13.13.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^3.4.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^6.2.2",
    "husky": "^9.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "sort-package-json": "^2.10.0",
    "start-server-and-test": "^2.0.5",
    "tailwindcss": "^3.4.7",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-svgr": "^4.2.0"
  }
}
